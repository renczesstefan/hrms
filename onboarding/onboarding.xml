    <data type="enumeration">
        <id>install_programs_tutorials</id>
        <title>Návod na inštaláciu programov</title>
        <values>Intellij IDEA</values>
        <values>Java 8</values>
        <values>Intellij WebStorm</values>
        <values>MongoDB v3</values>
        <values>Redis v4</values>
        <values>MySQL v5</values>
        <values>Neo4j</values>
        <values>Slack</values>
        <values>Chrome</values>
        <values>Elasticsearch 6.6.2</values>
        <values>Apache2</values>
        <values>Enterprise Architect</values>
        <action trigger="set">
            install_programs_tutorials : f.install_programs_tutorials,
            tutorial : f.tutorial;
            if((install_programs_tutorials.value as String).equals("Intellij IDEA")){
                change tutorial value { "sudo snap install intellij-idea-ultimate --classic" }
            }
            else if((install_programs_tutorials.value as String).equals("Java 8")){
                change tutorial value { "sudo -S add-apt-repository ppa:webupd8team/java -y" +
                "sudo apt-get update\n" +
                "echo debconf shared/accepted-oracle-license-v1-1 select true | sudo\n" +
                "debconf-set-selections\n" +
                "echo debconf shared/accepted-oracle-license-v1-1 seen true | sudo\n" +
                "debconf-set-selections\n" +
                "sudo apt-get install -y oracle-java8-installer\n" +
                "sudo echo 'JAVA_HOME=/usr/lib/jvm/java-8-oracle' >> /etc/environment\n" +
                "source /etc/environment"}
            }
            else if((install_programs_tutorials.value as String).equals("Intellij WebStorm")){
                change tutorial value { "sudo snap install webstorm --classic" }
            }
           else if((install_programs_tutorials.value as String).equals("MongoDB v3")){
                change tutorial value { "sudo apt-key adv --keyserver hkp://keyserver.ubuntu.com:80 --recv\n" +
                "2930ADAE8CAF5059EE73BB4B58712A2291FA4AD5\n" +
                "echo deb [ arch=amd64,arm64 ] https://repo.mongodb.org/apt/ubuntu\n" +
                "xenial/mongodb-org/3.6 multiverse | sudo tee\n" +
                "/etc/apt/sources.list.d/mongodb-org-3.6.list\n" +
                "sudo apt-get update\n" +
                "sudo apt-get install -y mongodb-org=3.6.8 mongodb-org-server=3.6.8\n" +
                "mongodb-org-shell=3.6.8 mongodb-org-mongos=3.6.8 mongodb-org-tools=3.6.8\n" +
                "sudo service mongod start" }
            }
            else if((install_programs_tutorials.value as String).equals("Redis v4")){
               change tutorial value { "sudo apt-get update\n" +
               "sudo apt-get install -y build-essential tcl\n" +
               "sudo add-apt-repository ppa:chris-lea/redis-server -y\n" +
               "sudo apt-get update\n" +
               "sudo apt-get install -y redis-server\n" +
               "sudo echo 'supervised systemd' >> /etc/redis/redis.conf\n" +
               "sudo echo 'dir /var/lib/redis' >> /etc/redis/redis.conf\n" +
               "sudo systemctl enable redis-server.service" }
           }
           else if((install_programs_tutorials.value as String).equals("MySQL v5")){
            change tutorial value { "sudo apt-get install mysql-client-5.7" }
           }
           else if((install_programs_tutorials.value as String).equals("Neo4j")){
               change tutorial value { "sudo su\n" +
               "wget -O- http://debian.neo4j.org/neotechnology.gpg.key | apt-key add\n" +
               "echo 'deb http://debian.neo4j.org/repo stable/' > /etc/apt/sources.list.d/neo4j.list\n" +
               "sudo apt-get update\n" +
               "sudo apt install -y neo4j\n" +
               "sudo systemctl enable neo4j.service\n" +
               "sudo systemctl start neo4j.service\n" +
               "curl -H 'Content-Type: application/json' -X POST -d '{'password':'netgrif_nae'}' -u neo4j:neo4j http://localhost:7474/user/neo4j/password\n" +
               "sudo echo 'NEO4J_ULIMIT_NOFILE=60000' >> /etc/default/neo4j" }
           }
           else if((install_programs_tutorials.value as String).equals("Slack")){
            change tutorial value { "sudo snap install slack --classic" }
           }
           else if((install_programs_tutorials.value as String).equals("Chrome")){
               change tutorial value { "wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb\n" +
               "sudo dpkg -i google-chrome-stable_current_amd64.deb" }
           }
           else if((install_programs_tutorials.value as String).equals("Elasticsearch 6.6.2")){
               change tutorial value { "sudo apt-get update\n" +
               "wget https://download.elastic.co/elasticsearch/release/org/elasticsearch/distribution/deb/elasticsearch/6.6.2/elasticsearch-6.6.2.deb\n" +
               "sudo dpkg -i elasticsearch-6.6.2.deb" }
           }
           else if((install_programs_tutorials.value as String).equals("Apache2")){
               change tutorial value { "sudo nano /etc/apache2/sites-available/nae.local.conf\n" +
               "sudo a2ensite nae.local\n" +
               "systemctl reload apache2\n" +
               "sudo nano /etc/hosts" }
           }
           else if((install_programs_tutorials.value as String).equals("Enterprise Architect")){
               change tutorial value { "sudo dpkg --add-architecture i386\n" +
               "sudo add-apt-repository ppa:wine/wine-builds\n" +
               "sudo apt-get update\n" +
               "sudo apt-get install --install-recommends winehq-devel\n" +
               "sudo apt-get install winetricks\n" +
               "sudo apt-get install fonts-crosextra-carlito\n" +
               "winetricks msxml3\n" +
               "winetricks msxml4\n" +
               "winetricks mdac28\n" +
               "rm -R ~/.wine\n" +
               "WINEPREFIX='$HOME/.wine' WINEARCH=win32 wine wineboot\n" +
               "winetricks jet40\n" +
               "wine msiexec /i /mnt/7A82239D82235CBF/Download/EALite.msi" }
           }
        </action>
    </data>
